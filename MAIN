from turtle import *
import math

pensize(10)

CURRENT = -20

def f(x):
    y = x#math.tan(x)*1
    return y

def m(x):
    if x >= 0:
        return x
    elif x < 0:
        return x*-1
    
x = CURRENT
penup()
goto(x*10, f(x)-100)
pendown()

for i in range(m(CURRENT*2)):
    x += 1
    goto(x*10, f(x)-100)

exitonclick()
